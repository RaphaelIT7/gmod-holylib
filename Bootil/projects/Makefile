# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug_x32
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x32)
  bootil_static_config = debug_x32

else ifeq ($(config),debug_x64)
  bootil_static_config = debug_x64

else ifeq ($(config),release_x32)
  bootil_static_config = release_x32

else ifeq ($(config),release_x64)
  bootil_static_config = release_x64

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := bootil_static

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

bootil_static:
ifneq (,$(bootil_static_config))
	@echo "==== Building bootil_static ($(bootil_static_config)) ===="
	@${MAKE} --no-print-directory -C . -f bootil_static.make config=$(bootil_static_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f bootil_static.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_x32"
	@echo "  debug_x64"
	@echo "  release_x32"
	@echo "  release_x64"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   bootil_static"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"